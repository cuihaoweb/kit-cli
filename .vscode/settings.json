{
    /* ========================================= eslint ========================================== */
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    // 工作目录
    "eslint.workingDirectories": [
        ".eslintrc.js",
        ".eslintignore"
    ],
    "files.associations": {
        "*.json": "jsonc"
    },
    "cSpell.words": [
        "antd",
        "fontawesome",
        "fortawesome",
        "fullhash",
        "gsap",
        "hljs",
        "imgs",
        "jsoneditor",
        "lazyload",
        "nodenext",
        "persistor",
        "qiankun",
        "Sider",
        "weixin"
    ],
    "svg.preview.background": "custom",
    "typescript.tsdk": "node_modules/typescript/lib",
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "favorites.resources": [
        {
            "filePath": "packages/m-thor-components/modules/gt-local-h5-local-life-channel",
            "group": "Default"
        },
        {
            "filePath": "packages/m-thor-components/components/gt-local-h5-first-popup",
            "group": "Default"
        }
    ],
    "workbench.tree.enableStickyScroll": true,
    "svelte.plugin.svelte.compilerWarnings": {
        "a11y-click-events-have-key-events": "ignore",
        "a11y-no-static-element-interactions": "ignore"
    }
}